<script type="text/javascript">
  var pm_app_url = "https://app.getpostman.com";

  /**
   * Authentication
   */
  $(function () {
    // Check if the user is signed in and add a class
    // to the body if so. This will hide elements with the
    // .hide-if-signedin class and show elements with
    // the .hide-if-signedout class.
    if (document.cookie.indexOf('getpostmanlogin') > -1) {
      $('body').addClass('signedin');
    }
  });

  /**
   * Navigation
   */
  $('body').on('click', '#menu-toggle', function () {
    $('body').toggleClass('nav-open');
  });

  $('body').on('click', '#menu-toggle-overlay', function () {
    $('body').removeClass('nav-open');
  });

  /**
   * Force blur of inputs when tapping outside, hides soft-keyboard
   * on touch devices
   */
  $(document).on('touchstart', function (e) {
    var thisNodeName = e.target.nodeName;

    if (thisNodeName === 'INPUT' || thisNodeName === 'TEXTAREA') {
      return;
    }

    document.activeElement.blur();
    $('input, textarea').blur();
  });

  /**
   * Auto-playing videos
   *
   * Just add a pm-auto-video class to the HTML5 video element
   * and the video will play when visible, and pause when not
   * visible.
   */
  $(function () {
    var $autoVideos = $('video.pm-auto-video'),
      scrollWatch,
      autoVideo = function() {
        $autoVideos.each(function() {
          var $this = $(this);

          if($this.is(':in-viewport')) {
            $this.get(0).play();
          } else {
            $this.get(0).pause();
          }
        });
      };

    $(document).on('scroll', function() {
      clearTimeout(scrollWatch);
      scrollWatch = setTimeout(autoVideo, 500);
    });
  });
</script>

    <script>

'use strict';
/* global instantsearch */

var search = instantsearch({
    appId: 'SDZ6C0ROMM',
    apiKey: 'e83c353e0b538a05ad8ea1d70c32b0dd',
    indexName: 'blog',
    urlSync: true,
    searchFunction: function(helper) {
        var searchResults = $('.search-results');
        var statsCount = $('#stats');
        if (helper.state.query === '') {
            searchResults.hide();
            statsCount.hide();
            return;
        }
        helper.search();
        searchResults.show();
        statsCount.show();
    }
});

search.addWidget(
    instantsearch.widgets.searchBox({
        container: '#search-box',
        placeholder: 'Search documentation',
//        poweredBy: true,
        cssClasses: {
            input: ["st-default-search-input"/*, "pm-form-control", "pm-white-control"*/]
        }
    })
);


search.addWidget(
    instantsearch.widgets.stats({
        container: '#stats',
        templates: {
            body: '<div>{{nbHits}} results</div>'
        },
        cssClasses: {
            body: ["text-right", "text-muted"]
        }
    })
);



var hitTemplate =
    '<div class="hit">' +
        '<div class="infos">' +
            '<div class="hit-title"><strong><a href={{site.pm.root}}/docs{{url}}>{{title}}</strong></a></div>' +
            '<div class="hit-excerpt">{{{_highlightResult.text.value}}}</div>'+
        '</div>' +
    '</div>';



var noResultsTemplate = 'No results found matching <strong>{{query}}</strong>.';


search.addWidget(
    instantsearch.widgets.hits({
        container: '#hits',
        templates: {
            empty: noResultsTemplate,
            item: hitTemplate
        },
        cssClasses: {
            root: ['doc-hits','search-results'],
            empty: 'empty-hits',
            item: 'doc-hits-hit'
        }
    })
);

var container = document.querySelector('#pagination');
var paginationWidget = instantsearch.widgets.pagination({
    container: '#pagination',
    cssClasses: {
        root: ['pagination', 'search-results'],
        active: 'active'
    }
});
var oldRender = paginationWidget.render;
paginationWidget.render = function(params) {
    var currentState = params.results;
    if(currentState.nbPages===0) container.style.display='none';
    else container.style.display = 'block';
    oldRender.apply(this, arguments);
};
search.addWidget(paginationWidget);

search.start();

$('.ais-search-box--reset').click(function() {
    $('.ais-search-box--input').val('');
});

</script>
  

